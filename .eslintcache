ct } from \"react-router-dom\";\nimport {connect} from 'react-redux';\nimport store from './Redux/store';\nimport './App.css';\n\nconst App = (props) => {\n  return (\n      <div className=\"App\">    \n      <Router>\n          <Header />\n          <Switch>\n            <Route exact path=\"/\">\n               {props.status ? <Redirect to=\"/dashboard\" /> : <Login /> } \n            </Route>\n            <Route path=\"/password/reset\">\n               <Reset />\n            </Route>\n            <Route path=\"/register\">\n               <Register />\n            </Route>\n            <Route path=\"/dashboard\">\n             {props.role === 1 ? <AdminDashboard /> : <UserDashboard />}\n            </Route>\n          </Switch>\n          <Footer />\n      </Router>  \n    </div>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    status : state.login.loginStatus,\n    role : state.login.userInfo.role_id\n  }\n}\n\nexport default connect(mapStateToProps)(App);\n","/home/alireza/Documents/web.design/react/test/src/reportWebVitals.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/store.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/rootReducer.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/index.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerReducer.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerActions.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerTypes.js",[],"/home/alireza/Documents/web.design/react/test/src/components/user/UserDashboard.js",["138"],"import React, { useEffect} from 'react';\nimport DashboardHeader from '../generic/DashboardHeader';\nimport UserSideNav from './UserSideNav';\nimport UserDashInfo from './UserDashInfo';\nimport MyTests from '../generic/MyTests';\nimport MyProfile from '../generic/MyProfile';\nimport Test from '../generic/Test';\nimport Assessments from '../generic/Assessments';\nimport Transaction from '../generic/Transaction';\nimport { Route, Switch } from \"react-router-dom\";\n\n\n\n\nconst UserDashboard = (props) => {\n\n\n  return (\n            <div className=\"dash-container\">\n                <div className=\"dashContent container\">\n                    <DashboardHeader />\n                    <Switch>\n                        <Route exact path=\"/dashboard\">\n                           <UserDashInfo />\n                        </Route>\n                        <Route path=\"/dashboard/MyTests\">\n                            <MyTests />\n                        </Route>\n                        <Route path=\"/dashboard/MyProfile\">\n                            <MyProfile />\n                        </Route>\n                        <Route path=\"/dashboard/Test/:id/:name\">\n                            <Test />\n                        </Route>\n                        <Route path=\"/dashboard/transaction\">\n                            <Transaction />\n                        </Route>\n                        <Route path=\"/dashboard/assessments\">\n                            <Assessments />\n                        </Route>\n                   </Switch>\n                </div>\n                <aside>\n                    <UserSideNav />\n                </aside>\n            </div>\n  )\n}\n\n\nexport default UserDashboard;","/home/alireza/Documents/web.design/react/test/src/components/user/UserSideNav.js",[],"/home/alireza/Documents/web.design/react/test/src/components/admin/AdminDashboard.js",["139"],"/home/alireza/Documents/web.design/react/test/src/components/admin/AdminSideNav.js",[],"/home/alireza/Documents/web.design/react/test/src/components/admin/UsersList.js",["140","141"],"/home/alireza/Documents/web.design/react/test/src/components/admin/AdminDashInfo.js",[],"/home/alireza/Documents/web.design/react/test/src/components/user/UserDashInfo.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/DashboardHeader.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/MyProfile.js",["142"],"import React, {useEffect, useState} from 'react';\nimport { connect } from 'react-redux';\nimport edit from '../image/2.png';\nimport axios from 'axios';\nimport {API} from '../generic/Api';\n\n\nconst MyProfile = (props) => {\n\n        const [password, setPassword] = useState(\"\");\n        const [name,setName] = useState(\"\");\n        const [age, setAge] = useState();\n        const [gender, setGender] = useState(\"\");\n        const [message, setMessage] = useState(\"اطلاعات خود را به صورت کامل وارد نمایید\")\n\n    const setPassHandler = (e) => {\n    setPassword(e.target.value)\n  }\n\n  const setNameHandler = (e) => {\n    setName(e.target.value)\n  }\n\n   const setAgeHandler = (e) => {\n    setAge(e.target.value)\n  }\n\n   const setGenderHandler = (e) => {\n    setGender(e.target.value)\n  }\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    console.log(name, password, age, gender);\n\n         axios.put(`${API}/api/users/${props.userInfo.id}`,{ \n           name: name, \n           age: age, \n           password: password,\n           gender: gender \n        })\n        .then(response => {\n            console.log(response);\n            setMessage(\"اطلاعات شما به درستی ثبت گردید\")\n        })\n        .catch(error => console.log(error))\n  }\n\n\n\n\n  return (\n    <div className=\"edit-container p-h\">\n        <div>\n        <p>{message}</p>\n        <img src={edit} alt=\"\" className=\"mt-h-2\"/>\n        </div>\n        <form className=\"edit-form rtl\" onSubmit={submitHandler}>\n            <label for=\"name\">نام و نام خانوادگی</label>\n            <input className=\"mt-h-1\" type=\"text\" name=\"name\" onChange={setNameHandler}/>\n            <p className=\"comment\">نام و نام خانوادگی خود را از این قسمت ویرایش کنید</p>\n            <label for=\"email\">آدرس ایمیل</label>            \n            <input className=\"mt-h-1\" type=\"email\" name=\"email\" value={props.userInfo.email} readonly/>\n            <p className=\"comment\">برای تغییر ایمیل به بخش فنی وبسایت پیام دهید.</p>            \n            <label for=\"age\">سن</label>\n            <input className=\"mt-h-1\" type=\"number\" name=\"age\" onChange={setAgeHandler}/>\n            <p className=\"comment\">سن خود را وارد کنید</p>            \n            <label for=\"male\">مرد</label>\n            <input type=\"radio\" name=\"gender\" value=\"1\" onClick={setGenderHandler}/>\n            <label for=\"female\">زن</label>\n            <input type=\"radio\" name=\"gender\" value=\"0\" onClick={setGenderHandler}/>\n            <p className=\"comment\">جنسیت خود را انتخاب کنید</p>            \n            <label for=\"pwd1\">رمز عبور</label>\n            <input className=\"mt-h-1\" type=\"password\" name=\"pwd1\" onChange={setPassHandler}/>\n            <p className=\"comment\">برای تغییر رمز عبور، رمز جدیدی را برای خود وارد کنید</p>            \n            <input type=\"submit\" id=\"submit\" value=\"ذخیره اطلاعات\" />\n        </form>\n    </div>\n  )\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        userInfo : state.login.userInfo\n    }\n}\n\n\nexport default connect(mapStateToProps)(MyProfile);","/home/alireza/Documents/web.design/react/test/src/components/generic/Test.js",["143"],"/home/alireza/Documents/web.design/react/test/src/components/generic/MyTests.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Transaction.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Header.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Footer.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/auth/Login.js",["144"],"/home/alireza/Documents/web.design/react/test/src/components/generic/auth/Reset.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/auth/Register.js",[],"/