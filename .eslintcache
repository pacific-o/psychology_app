re';\nimport './App.css';\n\nconst App = (props) => {\n  return (\n      <div className=\"App\">    \n      <Router>\n          <Header />\n          <Switch>\n            <Route exact path=\"/\">\n               {props.status ? <Redirect to=\"/dashboard\" /> : <Login /> } \n            </Route>\n            <Route path=\"/password/reset\">\n               <Reset />\n            </Route>\n            <Route path=\"/register\">\n               <Register />\n            </Route>\n            <Route path=\"/dashboard\">\n             {props.role === 1 ? <AdminDashboard /> : <UserDashboard />}\n            </Route>\n          </Switch>\n          <Footer />\n      </Router>  \n    </div>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    status : state.login.loginStatus,\n    role : state.login.role\n  }\n}\n\nexport default connect(mapStateToProps)(App);\n","/home/alireza/Documents/web.design/react/test/src/reportWebVitals.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/store.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/rootReducer.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/index.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerReducer.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerActions.js",[],"/home/alireza/Documents/web.design/react/test/src/Redux/register/registerTypes.js",[],"/home/alireza/Documents/web.design/react/test/src/components/user/UserDashboard.js",["139"],"import React, { useEffect} from 'react';\nimport DashboardHeader from '../generic/DashboardHeader';\nimport UserSideNav from './UserSideNav';\nimport UserDashInfo from './UserDashInfo';\nimport MyTests from '../generic/MyTests';\nimport MyProfile from '../generic/MyProfile';\nimport Test from '../generic/Test';\nimport Assessments from '../generic/Assessments';\nimport Transaction from '../generic/Transaction';\nimport { Route, Switch } from \"react-router-dom\";\n\n\n\n\nconst UserDashboard = (props) => {\n\n\n  return (\n            <div className=\"dash-container\">\n                <div className=\"dashContent container\">\n                    <DashboardHeader />\n                    <Switch>\n                        <Route exact path=\"/dashboard\">\n                           <UserDashInfo />\n                        </Route>\n                        <Route path=\"/dashboard/MyTests\">\n                            <MyTests />\n                        </Route>\n                        <Route path=\"/dashboard/MyProfile\">\n                            <MyProfile />\n                        </Route>\n                        <Route path=\"/dashboard/Test/:id/:name\">\n                            <Test />\n                        </Route>\n                        <Route path=\"/dashboard/transaction\">\n                            <Transaction />\n                        </Route>\n                        <Route path=\"/dashboard/assessments\">\n                            <Assessments />\n                        </Route>\n                   </Switch>\n                </div>\n                <aside>\n                    <UserSideNav />\n                </aside>\n            </div>\n  )\n}\n\n\nexport default UserDashboard;","/home/alireza/Documents/web.design/react/test/src/components/user/UserSideNav.js",[],"/home/alireza/Documents/web.design/react/test/src/components/admin/AdminDashboard.js",["140"],"import React , { useEffect} from 'react';\nimport { Route, Switch } from \"react-router-dom\";\nimport DashboardHeader from '../generic/DashboardHeader';\nimport AdminSideNav from './AdminSideNav';\nimport AdminDashInfo from './AdminDashInfo';\nimport UsersList from './UsersList';\n\n\n\n\nconst AdminDashboard = (props) => {\n\n\n  return (\n            <div className=\"dash-container container\">\n                <div className=\"dash-content \">\n                    <DashboardHeader />\n                    <Switch>\n                        <Route exact path=\"/dashboard\">\n                           <AdminDashInfo />\n                        </Route>\n                        <Route path=\"/dashboard/UsersList\">\n                            <UsersList />\n                        </Route>\n                   </Switch>\n                </div>\n                <aside>\n                    <AdminSideNav />\n                </aside>\n            </div>  )\n}\n\n\n\nexport default AdminDashboard;","/home/alireza/Documents/web.design/react/test/src/components/admin/AdminSideNav.js",[],"/home/alireza/Documents/web.design/react/test/src/components/admin/UsersList.js",["141","142"],"import React, {useEffect, useState} from 'react';\nimport Loader from '../generic/Loader';\n\nconst UsersInfo = (props) => {\n\n  const [loader, setLoader] = useState(false);\n\tconst[list, setUsersList] = useState([]);\n  const[selectedUser, setSelectedUser] = useState([]);\n\n  useEffect(() => {\n        setLoader(true);\n        fetch(`http://37.152.178.76:54000/api/users`)\n        .then(res => res.json())\n        .then(data => {\n           setLoader(false);\n           setUsersList(data.data);\n        });\n    }, []);\n\n  const selectorHandler = (e) => {\n    list.map(item => {\n      if(item.id == e.target.id){\n        setSelectedUser(item)\n      }\n    });\n    showSelected();\n  }\n\n  const showSelected = () => {\n        document.querySelector(\".hidden-user-info\").classList.toggle(\"show-info\");\n  }\n\n\n  return ( loader ? <Loader /> :\n    <div className=\"users-container container mt-h-1\">\n      <table>\n        <thead>\n          <tr>\n            <th colSpan=\"3\">ویرایش</th>\n            <th>سطح کاربری</th>\n            <th>ایمیل</th>\n          </tr>\n         </thead>\n         <tbody> \n        {list.map(item => (\n          <tr>\n            <td><button className=\"btn btn-danger\" id={item.id}>حذف</button></td>\n            <td><button className=\"btn btn-success\" id={item.id} onClick={showSelected}>ویرایش</button></td>\n            <td><button className=\"btn btn-info\" id={item.id} onClick={selectorHandler}>مشاهده</button></td>\n            <td>{item.role}</td>\n            <td>{item.email}</td>\n          </tr>\n          ))}\n        </tbody>\n     </table>\n     <div className=\"hidden-user-info\">\n      <form className=\"mt-h-4\">\n       <div className=\"user-info-container\">\n        <div className=\"row\">\n          <div className=\"rtl\">\n            <label for=\"name\">نام</label>\n            <input className=\"m-h-1\" type=\"text\" name=\"name\" value={selectedUser.name}/>\n          </div>\n          <div className=\"rtl\">\n            <label for=\"email\">ایمیل</label>\n            <input className=\"m-h-1\" type=\"email\" name=\"email\" value={selectedUser.email}/>\n          </div>\n          <div className=\"rtl\">\n            <label for=\"age\">سن</label>\n            <input className=\"m-h-1\" type=\"text\" name=\"age\" value={selectedUser.age}/>\n          </div>                    \n        </div>\n        <div className=\"row\">\n          <div className=\"rtl\">\n            <label for=\"gender\">جنسیت</label>\n            <input className=\"m-h-1\" type=\"text\" name=\"gender\" value={selectedUser.gender}/>\n          </div>\n          <div className=\"rtl\">\n            <label for=\"phone\">شماره تلفن</label>\n            <input className=\"m-h-1\" type=\"text\" name=\"phone\" value={selectedUser.phone}/>\n          </div>\n          <div className=\"rtl\">\n            <label for=\"role\">سطح دسترسی</label>\n            <input className=\"m-h-1\" type=\"text\" name=\"role\" value={selectedUser.role}/>\n          </div>                    \n        </div> \n       </div>       \n      </form>\n      <button onClick={showSelected}>خروج</button>\n     </div>\n    </div>\n  )\n}\n\nexport default UsersInfo;","/home/alireza/Documents/web.design/react/test/src/components/admin/AdminDashInfo.js",[],"/home/alireza/Documents/web.design/react/test/src/components/user/UserDashInfo.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/DashboardHeader.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/MyProfile.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Test.js",["143"],"import React , {useEffect, useState} from 'react';\nimport { useParams } from \"react-router-dom\";\nimport Loader from './Loader';\n\n\n\n\nconst TestsList = (props) => {\n\n      const [loader, setLoader] = useState(false)\n      let { id } = useParams();\n\n\n  const [data, setQustionsData] = useState([])\n  const [index, setActiveIndex] = useState(1)\n\n  useEffect(() => {\n     setLoader(true);\n     fetch(`http://37.152.178.76:54000/api/assessments/${id}`)\n     .then(res => res.json())\n     .then(data => {\n      console.log(data);\n      console.log(data.data.questions);\n      setLoader(false);\n      setQustionsData(data.data.questions);\n      setActiveCard(index);\n  })\n  },[])\n\n \n  const setActiveCard = (index) => {\n   let card = document.querySelectorAll('.card');\n    card[index - 1].classList.add(\"active\")\n  }\n\n\n  const nextCard = () => {\n\n    if (index === data.length) {\n      return\n    } else {\n\n    let newIndex = index + 1;\n    setActiveIndex(newIndex);\n\n    let activeCard = document.querySelector('.active');\n    activeCard.classList.remove(\"active\");\n    setActiveCard(newIndex);\n\n    let cardsEl = document.querySelector('.cards');\n    let percent = \"-\" + ((newIndex - 1) * 100) + \"%\";\n    cardsEl.style.transform = `translateX(${percent})`;\n    }\n\n  }\n\n\n  const preCard = () => {\n   if (index === 1) {\n    return\n   } else {\n\n    let newIndex = index - 1;\n    setActiveIndex(newIndex);\n\n    let activeCard = document.querySelector('.active');\n    activeCard.classList.remove(\"active\");\n    setActiveCard(newIndex);\n\n    let cardsEl = document.querySelector('.cards');\n    let percent = \"-\" + ((newIndex - 1 ) * 100) + \"%\";\n    cardsEl.style.transform = `translateX(${percent})`;\n   }\n\n  }\n\n\n  const answerHandle = (e) => {\n    console.log(e.target.parentNode.parentNode.parentNode.getAttribute('questionNum'), e.target.value);\n    nextCard();\n  }\n\n\n  const questionCard = data.map(item => (\n     <div className=\"card\" questionNum={item.id}>\n        <div className=\"question-container\"><p className=\"question\">{item.text}</p></div>\n        <div className=\"answer p-h\" style={{gridTemplateColumns: `repeat(${item.answers.length}, 1fr)`}}>\n            {item.answers.map(answer => (\n              <div>\n                <input type=\"radio\" name=\"answer\" value={answer.value} onClick={answerHandle}/>\n                <label for={answer.text}>{answer.text}</label>\n             </div>\n            ))}\n        </div>\n     </div>\n   \n  \t))\n\n\n\n  return ( loader ? <Loader /> :\n    <div className=\"cards-container\">\n      <div className=\"cards\" style={{gridTemplateColumns: `repeat(${data.length}, 1fr)`}}>\n         {questionCard}\n      </div>\n      <div className=\"card-navigation mt-h-2\">\n         <span><i className=\"fas fa-arrow-left\" aria-hidden=\"true\" onClick={preCard}></i></span>\n         <span>{index}</span>\n         <span>/</span>\n         <span>{data.length}</span>\n         <span><i className=\"fas fa-arrow-right\" aria-hidden=\"true\" onClick={nextCard}></i></span>       \n         </div>\n    </div>\n  )\n}\n\nexport default TestsList;","/home/alireza/Documents/web.design/react/test/src/components/generic/MyTests.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Transaction.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Header.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/Footer.js",[],"/home/alireza/Documents/web.design/react/test/src/components/generic/auth/Login.js",["144"],"import React, { useEffec